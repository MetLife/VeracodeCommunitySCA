{
    "manifestVersion": 1,
    "id": "VeracodeCommunityScaAdoExtension",
    "name": "Veracode Community SCA Azure DevOps Extension",
    "version": "1.0.0",
    "publisher": "gattjoe",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Veracode Community SCA Azure DevOps Extension",
    "tags": [
        "Security",
        "Azure Pipelines",
        "Azure DevOps Extensions",
        "Software Composition Analysis",
        "Veracode",
        "SCA"
      ],
    "categories": [
        "Azure Pipelines"
    ],
    "links": {
        "support": {
          "uri": "mailto:joe@metlife.com"
        }
      },
    "repository": {
        "type": "git",
        "uri": "https://github.com/MetLife/VeracodeCommunitySCA"
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        },
        {
            "path": "images", "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}